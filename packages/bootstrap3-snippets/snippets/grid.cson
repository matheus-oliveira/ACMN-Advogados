'.text.html':

  # Row
  'Row':
    'prefix': 'bs3-row'
    'body': """
      <div class="row">
	     ${0}
      </div>
    """

  'Container':
    'prefix': 'bs3-container'
    'body': """
      <div class="container">
	     ${0}
      </div>
    """
    # All col's snippets
  'Col':
    'prefix': 'bs3-col'
    'body': """
      <div class="${2:col-xs-${1}} ${3:col-sm-${1}} ${4:col-md-${1}} ${5:col-lg-${1}}">
	     ${0}
      </div>
    """

  
