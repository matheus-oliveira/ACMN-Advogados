'.text.html.php.blade, .text.html, .text.html.php':

  'Bulma layout - Container':
    prefix: 'bulma:container'
    leftLabelHTML: '<span style="color:#1B81B6">Ⓢ</span>'
    rightLabelHTML: '<span style="color:#00d1b2">Bulma.io</span> Container'
    body: """
      <div class="container is-fluid">
        <div class="notification">
          This container is <strong>fluid</strong>: it will have a 24px gap on either side, on any viewport size.
        </div>
      </div>
    """

